<UserControl x:Class="DataLogger.Views.Home_V"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns:local="clr-namespace:DataLogger.Views"
                   mc:Ignorable="d"
                   Height="600" Width="1200">

    <UserControl.Content>
        <StackPanel Orientation="Horizontal">
            <StackPanel MinWidth="100">
                <ComboBox ItemsSource="{Binding UserNames}" Text="{Binding SelectedUserName, Mode=TwoWay, UpdateSourceTrigger=LostFocus}" IsEditable="True"/>
            </StackPanel>
        </StackPanel>
    </UserControl.Content>
</UserControl>
