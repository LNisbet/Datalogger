<NavigationWindow x:Class="DataLogger.Views.MainWindow_V"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns:local="clr-namespace:DataLogger.Views"
                   xmlns:uc="clr-namespace:DataLogger.Views.UserControls"
                   mc:Ignorable="d"
                   Title="MainWindow" Height="600" Width="1200"
                   Closing="WindowClosing">
    
    <NavigationWindow.Content>
        <StackPanel Orientation="Horizontal">
            <local:Navigation_Control/>
            <StackPanel MinWidth="100">
                <ComboBox ItemsSource="{Binding UserNames}" Text="{Binding SelectedUserName, Mode=TwoWay, UpdateSourceTrigger=LostFocus}" IsEditable="True"/>
            </StackPanel>
        </StackPanel>
    </NavigationWindow.Content>
</NavigationWindow>
