<Page x:Class="DataLogger.Views.CSV_V"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DataLogger.Views"
      xmlns:uc="clr-namespace:DataLogger.Views.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="1200"
      Title="CSV_V">
    
    <StackPanel Orientation="Horizontal">
        <local:Navigation_Control/>
        <StackPanel>
            <Button Width="100" Content="Import Logs" Command="{Binding Path=ImportLogsCommand}"/>
            <Button Width="100" Content="Export Logs" Command="{Binding Path=ExportLogsCommand}"/>
            <Button Width="100" Content="Import Exercises" Command="{Binding Path=ImportExercisesCommand}"/>
            <Button Width="100" Content="Export Exercises" Command="{Binding Path=ExportExercisesCommand}"/>
        </StackPanel>
    </StackPanel>
</Page>
